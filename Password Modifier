word = input()
password = ''

''' The key  
i becomes 1
a becomes @
m becomes M
B becomes 8
s becomes $'''

for x in word:

    word = word.replace("i", "1")

    word = word.replace("a", "@")

    word = word.replace("s", "$")

    word = word.replace("m", "M")

    word= word.replace("B", "8")

print('{}!'.format(word))
